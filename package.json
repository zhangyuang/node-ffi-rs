{
  "name": "ffi-rs",
  "version": "1.0.22",
  "main": "index.js",
  "types": "index.d.ts",
  "description": "A module written in Rust and N-API provides interface (FFI) features for Node.js",
  "napi": {
    "name": "ffi-rs",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl"
      ]
    }
  },
  "author": "zhangyuang",
  "homepage": "https://github.com/zhangyuang/node-ffi-rs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhangyuang/node-ffi-rs.git"
  },
  "keywords": [
    "ffi",
    "rust",
    "node.js",
    "napi"
  ],
  "files": [
    "index.js",
    "index.d.ts",
    "README.md"
  ],
  "license": "MIT",
  "dependencies": {
    "@napi-rs/cli": "^2.15.2",
    "esno": "^0.17.0",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "benny": "^3.7.1",
    "ffi-napi": "^4.0.3",
    "koa": "^2.14.2"
  },
  "engines": {
    "node": ">= 12"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build:c": "node scripts/compile.js",
    "build:dev": "env=development node scripts/build.js",
    "build": "node scripts/build.js",
    "build:debug": "napi build --platform",
    "publish:npm": "node scripts/publish.js && napi prepublish -t npm",
    "test": "esno ./test.ts",
    "universal": "napi universal",
    "version": "napi version"
  },
  "optionalDependencies": {
    "@yuuang/ffi-rs-darwin-arm64": "^1.0.6",
    "@yuuang/ffi-rs-darwin-x64": "^1.0.6",
    "@yuuang/ffi-rs-linux-arm64-gnu": "^1.0.6",
    "@yuuang/ffi-rs-linux-arm64-musl": "^1.0.6",
    "@yuuang/ffi-rs-linux-x64-gnu": "^1.0.6",
    "@yuuang/ffi-rs-win32-x64-msvc": "^1.0.6"
  }
}
